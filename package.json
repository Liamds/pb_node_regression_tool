{
  "name": "agilereporter-variance-analysis",
  "version": "1.0.0",
  "description": "Variance analysis tool for AgileReporter API",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && vite build",
    "build:server": "tsc",
    "start": "node dist/main.js",
    "dev": "tsx src/main.ts",
    "dev:dashboard": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx src/dashboard-cli.ts",
    "dev:client": "vite",
    "dashboard": "npm run clean && npm run build:server && npm run build && node dist/dashboard-cli.js -p 5000",
    "dashboard:dev": "tsx src/dashboard-cli.ts",
    "dashboard:build": "node dist/dashboard-cli.js",
    "check-env": "tsx src/check-env.ts",
    "debug-auth": "tsx src/debug-auth.ts",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist",
    "preview": "vite preview"
  },
  "keywords": [
    "variance-analysis",
    "agilereporter",
    "excel",
    "reporting",
    "api-documentation",
    "swagger"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^5.90.12",
    "@trpc/client": "^11.7.2",
    "@trpc/react-query": "^11.7.2",
    "@trpc/server": "^11.7.2",
    "ansi-colors": "^4.1.3",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "cli-progress": "^3.12.0",
    "clsx": "^2.1.1",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.2.1",
    "json-2-csv": "^5.5.10",
    "lucide-react": "^0.556.0",
    "p-limit": "^7.2.0",
    "recharts": "^3.5.1",
    "sql.js": "^1.13.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "eslint": "^9.39.1",
    "postcss": "^8.5.6",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "rimraf": "^6.1.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.7"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
